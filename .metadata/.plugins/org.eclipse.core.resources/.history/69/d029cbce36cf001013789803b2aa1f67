package com.example.studentCourses.repository;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.example.studentCourses.Entity.RegisteredStudentsEntity;

@Repository
public interface RegisteredStudentsRepository extends JpaRepository<RegisteredStudentsEntity,Long> {
	 RegisteredStudentsEntity findBySEmail(String sEmail);
}
