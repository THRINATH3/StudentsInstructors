package com.example.studentCourses.repository;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.example.studentCourses.Entity.RegisteredInstructorsEntity;

@Repository
public interface RegisteredInstructorsRespository extends JpaRepository<RegisteredInstructorsEntity, Long> {

	public RegisteredInstructorsEntity findByIEmail(String IEmail);
	
}
